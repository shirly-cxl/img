.head{
	background-color: rgb(254,247,238);
	width: 100%;
}
.background{
	width: 100%;
	position: relative;
	top: 0;
	left: 0;
	z-index: 1;
}
.userInfo{
	position: absolute;
	top: 0;
	left: 35%;
	z-index: 999;
}
.bg{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.img-bg{
	width: 170rpx;
	height: 170rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	background-color: #000000;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin:40rpx 0;
	position: relative;
}
.img{
	width: 100rpx;
	height: 100rpx;
	position: relative;
	top: 24rpx;
	left: 0;
}
.line{
	width: 230rpx;
	margin-top: 20rpx;
	margin: 0 auto;
}
.login{
	background-color: #000000;
	font-size: 14px;
	color: white;
	-webkit-border-radius: 5px;
	        border-radius: 5px;
	text-align: center;
	padding: 0rpx 20rpx;
}
.mine{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background-color: rgba(0,0,0,0.1);
	color: #000;
	padding: 20rpx 40rpx;
	margin: 25rpx 0;
}
.list{
	width: 80rpx;
	height: 80rpx;
}
.name{
	font-size: 18px;
	margin-left: 25rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.arrow{
	width: 30rpx;
	height: 30rpx;
}
.close{
	background-color: rgba(0,0,0,0.2);
	width: 100%;
	height: 120rpx;
	line-height: 120rpx;
	color: #000;
	font-weight: 600;
	font-size: 20px;
	text-align: center;	
	margin-top: 50rpx;
}
.act{
	width: 170rpx;
	height: 170rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	margin:40rpx 0;
}
.true{
	font-size: 16px;
	color: #000000;
	text-align: center;
	padding: 5rpx 10rpx;
}

